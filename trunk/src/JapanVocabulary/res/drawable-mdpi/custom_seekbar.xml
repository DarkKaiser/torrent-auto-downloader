<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="15dp" />
			<stroke android:width="4dp" android:color="#FF000000" />
			<gradient android:startColor="#42C3FF" android:centerColor="#0096FF"
				android:endColor="#216db5" android:angle="270" android:centerY="0.6" />
		</shape>
	</item>
	<item android:id="@android:id/secondaryProgress">
		<clip>
			<shape>
				<corners android:radius="15dp" />
				<gradient android:startColor="#80FFD300"
					android:centerColor="#80FFB600" android:angle="270"
					android:centerY="0.75" android:endColor="#A0FFCB00" />
			</shape>
		</clip>
	</item>
	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<corners android:radius="15dp" />
				<stroke android:width="4dp" android:color="#FF000000" />
				<gradient android:startColor="#42C3FF" android:centerColor="#0096FF"
					android:endColor="#216db5" android:angle="270" android:centerY="0.6" />
			</shape>
		</clip>
	</item>
</layer-list>
